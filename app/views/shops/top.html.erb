  <div class="col-md-10 col-md-offset-1">
    <h2>レストランを探す</h2>
  </div>
</div>
<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <div class="row m-lr-20">
      <div class="col-md-3">
        <%= image_tag '/images/iwate.jpg', class: "img-responsive" %>
      </div>
      <div class="col-md-3">
        <%= image_tag '/images/tokyo.jpg', class: "img-responsive" %>
      </div>
      <div class="col-md-3">
        <%= image_tag '/images/osaka.jpg', class: "img-responsive" %>
      </div>
      <div class="col-md-3">
        <%= image_tag '/images/hiroshima.jpg', class: "img-responsive" %>
      </div>
    </div>
  </div>
</div>
<div class="row">
    <%= search_form_for @q do |f| %>
      <div class="input-group">
        <span class="input-group-btn">
          <%= f.submit "口コミ投稿", class: "btn btn-warning pull-right" %>
        </span>
      </div>
    <% end %>
</div>
